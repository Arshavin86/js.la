div.jsla-toplevel-container.jsla-event-container
    h1  #{event.datetime}

div.jsla-toplevel-container.wide.jsla-past-event
    div.jsla-toplevel-container
      ul.jsla-speakers-list
          each speaker in event.speakers
              != partial("../components/speakers/_speaker", speaker )

    != partial( "../components/_sponsors-box", { sponsors: event.sponsors } )

    if event.flickr
        div.jsla-toplevel-container
            div.jsla-event-photos.jsla-content-section.layer-2( data-flickr=event.flickr )
                h2 Event Photos
